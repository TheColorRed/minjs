// Place your settings in this file to overwrite default and user settings.
{
    "json.schemas": [
        {
            "fileMatch": [ "config/minjs.json" ],
            "url": "./src/json.schemas/config.schema.json"
        },
        {
            "fileMatch": [ "config/routes/**.json" ],
            "url": "./src/json.schemas/routes.schema.json"
        },
        {
            "fileMatch": [ "config/themes/**.json" ],
            "url": "./src/json.schemas/themes.material.schema.json"
        },
        {
            "fileMatch": [ "config/animation/**.json" ],
            "url": "./src/json.schemas/animations.schema.json"
        }
    ]
}